{
  "expo": {
    "name": "dermaland",
    "slug": "dermaland",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userPoppingsfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EADDD3"
    },
    "notification": {
      "icon": "./assets/notif_icon_grayscale.png",
      "color": "#000000",
      "androidMode": "default",
      "iosDisplayInForeground": true,
      "androidCollapsedTitle": "Reminder from Dermaland"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EADDD3"
      },
      "permissions": ["RECEIVE_BOOT_COMPLETED"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you change your profile photo.",
          "sounds": ["./assets/sound/dermaland-magic-notification-ring.wav"]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notif_icon_grayscale.png",
          "color": "#000000",
          "sounds": ["./assets/sound/dermaland-magic-notification-ring.wav"]
        }
      ]
    ]
  }
}
